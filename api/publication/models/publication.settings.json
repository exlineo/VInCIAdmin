{
  "connection": "default",
  "collectionName": "publications",
  "info": {
    "name": "publication"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titre": {
      "type": "string"
    },
    "alias": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "lien": {
      "type": "string"
    },
    "document": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "langue": {
      "model": "langue"
    }
  }
}
