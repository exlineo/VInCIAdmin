{
  "connection": "default",
  "collectionName": "sitemaps",
  "info": {
    "name": "menu"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titre_fr": {
      "type": "string"
    },
    "chemin": {
      "type": "string"
    },
    "menu": {
      "type": "enumeration",
      "enum": [
        "principal",
        "pied"
      ]
    },
    "icone": {
      "type": "string"
    },
    "couleurGauche": {
      "model": "couleur"
    },
    "couleurDroite": {
      "model": "couleur"
    },
    "ordre": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "commun",
        "divers",
        "vincis"
      ]
    },
    "medias": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "langue": {
      "model": "langue"
    },
    "titre_pt": {
      "type": "string"
    },
    "titre_es": {
      "type": "string"
    },
    "titre_en": {
      "type": "string"
    }
  }
}
